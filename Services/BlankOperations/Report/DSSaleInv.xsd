                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT ABS(CAST(ISNULL(A.NETAMOUNT,0) AS DECIMAL(28,2))) AS NETAMOUNT
		,ABS(CAST(ISNULL(A.GROSSAMOUNT,0) AS DECIMAL(28,2))) AS GROSSAMOUNT 
